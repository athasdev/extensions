{
  "$schema": "https://athas.dev/schemas/extension.json",
  "id": "athas.csharp",
  "name": "CSharp",
  "displayName": "C#",
  "version": "1.0.0",
  "description": "C# language support with syntax highlighting",
  "publisher": "Athas",
  "categories": ["Language"],
  "languages": [
    {
      "id": "csharp",
      "extensions": [".cs"],
      "aliases": ["C#", "CSharp", "csharp"]
    }
  ],
  "capabilities": {
    "grammar": {
      "wasmPath": "parser.wasm",
      "highlightQuery": "highlights.scm"
    }
  }
}
